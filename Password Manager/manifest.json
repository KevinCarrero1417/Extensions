{
    "manifest_version": 3,
    "name": "Password manager",
    "description": "Administrador de contrase√±as",
    "version": "1",
    "icons": {
        "128": "img/icon.png"
    },
    "permissions": [
        "activeTab",
        "storage"
    ],
    "action": {
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "js": [
                "content.js"
            ]
        }
    ]
}